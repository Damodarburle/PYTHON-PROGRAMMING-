def palindrome(n):
  original=n
  sum=0
  while n>0:
    r=n%10
    sum=sum*10+r
    n=n//10
  if sum==original:
    return True
   
  else:
    return False
   
       
input=int(input("Enter the number:"))
if palindrome(input):
  print("It is a palindrome")

else:
  print("It is not a palindrome")
